<Window x:Class="Nalice.smallWindow.AleartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:Nalice.smallWindow"
        mc:Ignorable="d"
        Background="#202023"
        Title="AleartWindow" Height="174.82" Width="333.813" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="submit_button" Content="{materialDesign:PackIcon CheckUnderline}" ToolTip="Icon" Margin="10,99,10,0" Height="31" Background="#FF00E8FF" Foreground="#DD000000" Click="submit_button_Click" VerticalAlignment="Top" />
        <Label x:Name="aleart_label" Content="Aleart" HorizontalAlignment="Left" Margin="10,20,0,0" Foreground="#DDFFFFFF" Width="308" Height="57" VerticalAlignment="Top"/>
    </Grid>
</Window>
